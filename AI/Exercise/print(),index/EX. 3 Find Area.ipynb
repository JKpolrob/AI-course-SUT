    "#Find Area of triangle\n",
    "import math\n",
    "\n",
    "a = 5\n",
    "b = 6\n",
    "c = 7\n",
    "s = (a+b+c)/2\n",
    "AREA = math.sqrt(s*(s-a)*(s-b)*(s-c))\n",
    "print('The area of the triangle is {:,.2f}'.format(AREA))"
   ]
